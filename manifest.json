{
  "name": "EDUrate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension rates YouTube videos on their educational value.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "clipboardRead",
    "contentSettings",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
