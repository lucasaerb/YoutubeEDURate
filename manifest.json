{

  "name" : "EDUrate",
  "manifest_version" : 2,
  "version" : "0.0.1",
  "description" : "An extension designed to rate YouTube videos on its educational value",

  "browser_action" :{
    "default_icon" : "icons/icon19.png"
},

  "content_scripts":[
    {
    "matches" : [
      "https://www.youtube.com/*"

    ],
    "js" : [
      "src/content.js"
    ]
  }
  ],
  "background":{
    "scripts" : ["src/background.js"]
  }

}
